# Nama aplikasi Laravel yang akan digunakan
APP_NAME=Laravel

# Environment aplikasi (local, staging, production)
APP_ENV=local

# Kunci enkripsi aplikasi untuk keamanan (jangan ubah secara manual)
APP_KEY=

# Mengaktifkan mode debugging (true untuk development, false untuk production)
APP_DEBUG=true

# Zona waktu aplikasi (disesuaikan dengan wilayah pengguna)
APP_TIMEZONE=Asia/Jakarta

# URL utama aplikasi Laravel
APP_URL=http://localhost

# Bahasa utama aplikasi yang digunakan dalam tampilan
APP_LOCALE=en

# Bahasa cadangan jika bahasa utama tidak tersedia
APP_FALLBACK_LOCALE=en

# Bahasa yang digunakan oleh Faker untuk generate data dummy
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

# Jenis database yang digunakan (MySQL, PostgreSQL, SQLite, dll.)
DB_CONNECTION=mysql

# Host atau alamat server database (biasanya 127.0.0.1 untuk lokal)
DB_HOST=127.0.0.1

# Port yang digunakan oleh MySQL (default: 3306)
DB_PORT=3306

# Nama database yang akan digunakan untuk aplikasi ini
DB_DATABASE=todolist

# Username untuk mengakses database (default: root di XAMPP/MAMP)
DB_USERNAME=root

# Password untuk mengakses database (kosong secara default di XAMPP)
DB_PASSWORD=


SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"
